import pandas as pd
from sqlalchemy import create_engine

# Define the connection parameters
host = 'redshift-cluster-cis9440-hws.chhiccjmjzfh.us-east-2.redshift.amazonaws.com'
port = '5439'
database = 'dev'
user = 'awsuser'
password = 'TWxsp3hrkN4eUt4'

# Create a connection string
connection = f'postgresql://{user}:{password}@{host}:{port}/{database}?standard_conforming_strings=off'

# Create a SQLAlchemy engine
engine = create_engine(connection)

# Define function to insert data into Redshift tables
def insert_into_redshift(table_name, data_df):
    try:
        # Insert data into the table using Pandas' to_sql function
        data_df.to_sql(table_name, engine, if_exists='append', index=False)
        print(f'Data inserted into {table_name} successfully.')
    except Exception as e:
        print(f'Error inserting data into {table_name}: {str(e)}')

# Load transformed data from CSV file into a DataFrame
transformed_data_df = pd.read_csv('transformed_bbp_data.csv')

# Split the DataFrame into separate DataFrames for each table
data_df_dim_time = transformed_data_df[['date', 'year', 'quarter', 'month', 'day', 'hour']].copy()
data_df_dim_weather = transformed_data_df[['weather', 'temperature', 'precipitation']].copy()
data_df_dim_event = transformed_data_df[['events']].copy()
data_df_fact_pedestrian = transformed_data_df[['date', 'weather', 'events', 'pedestrians', 'towards_manhattan', 'towards_brooklyn']].copy()

# Insert data into Redshift tables
insert_into_redshift('dim_time', data_df_dim_time)
insert_into_redshift('dim_weather', data_df_dim_weather)
insert_into_redshift('dim_event', data_df_dim_event)
insert_into_redshift('fact_pedestrian', data_df_fact_pedestrian)



